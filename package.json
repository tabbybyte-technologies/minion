    "ai": "^3.0.0",
    "dotenv": "^16.4.0",
    "ora": "^8.2.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md"
